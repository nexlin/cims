<Setup>
	<Sip>
		<LocalIp>0.0.0.0</LocalIp>
		<UdpPort>5060</UdpPort>
		<UdpThreadCount>1</UdpThreadCount>
		<TcpPort>25061</TcpPort>
		<TcpThreadCount>2</TcpThreadCount>
		<TcpRecvTimeout>600</TcpRecvTimeout>
		<Realm>csp</Realm>
		<!--<SendOptionsPeriod>30</SendOptionsPeriod>-->
		<TlsPort>5061</TlsPort>
		<TlsAcceptTimeout>10</TlsAcceptTimeout>
		<CertFile>../cert/csp.pem</CertFile>
		<MinRegisterTimeout>60</MinRegisterTimeout>
		<CallPickupId>**</CallPickupId>
		<StackExecutePeriod>20</StackExecutePeriod>
	</Sip>
	<RtpRelay>
		<UseRtpRelay>true</UseRtpRelay>
		<BeginPort>10000</BeginPort>
		<EndPort>12000</EndPort>
	</RtpRelay>
	<Log>
		<Folder>log</Folder>
		<Level Debug="true" Info="true" Network="true" Sql="true" />
		<MaxSize>10000000</MaxSize>
	</Log>
	<XmlFolder>
		<User>../config/UserXml</User>
		<SipServer>../config/SipServerXml</SipServer>
	</XmlFolder>
	<Cdr>
		<Folder>../log/cdr</Folder>
	</Cdr>
	<Monitor>
	  <Port>16000</Port>
	  <ClientIpList>
	    <ClientIp>127.0.0.1</ClientIp>
	    <ClientIp>192.168.0.30</ClientIp>
	  </ClientIpList>
	</Monitor>
	<Security>
		<DenySipUserAgentList>
			<SipUserAgent>friendly-scanner</SipUserAgent>
			<SipUserAgent>sundayddr</SipUserAgent>
		</DenySipUserAgentList>
	</Security>
</Setup>
